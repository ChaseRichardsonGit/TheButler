<!DOCTYPE html>
<html data-bs-theme="dark">
	<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>   <link rel="stylesheet" href="admin.css"> 
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.13.3/css/jquery.dataTables.min.css"/>
    <script type="text/javascript" src="https://cdn.datatables.net/1.13.3/js/jquery.dataTables.min.js"></script>
    <link rel="stylesheet" type="text/css" href="/admin.css"/>
	<body>
	<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
		<div class="container-fluid">
		  <a class="navbar-brand" href="/admin.html">Admin Panel</a>
		  <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		  </button>
		  <div class="collapse navbar-collapse" id="navbarNav">
			<ul class="navbar-nav ms-auto">
			  <li class="nav-item">
				<a class="nav-link btn btn-primary" href="/personas.html">Personas</a>
			  </li>
			  <li class="nav-item">
				<a class="nav-link btn btn-primary" href="/history.html">Chat History</a>
			  </li>
			</ul>
		  </div>
		</div>
	  </nav>
	<title><%= sender %> Statistics</title>
	<h1><%= sender %> Statistics</h1>
	<p>Total Messages: <%= totalMessages %></p>
	<p>Total Cost: $<%= totalCost %></p>
	<p>Total Tokens Used: <%= totalTokensUsed %></p>
	<!-- <p>Persona: <%= persona %></p>
	<p>Characters: <%= characters %></p>
	<p>Total Tokens: <%= total_tokens %></p>
	<p>Prompt Tokens: <%= prompt_tokens %></p>
	<p>Completion Tokens: <%= completion_tokens %></p>
	<p>Max Tokens: <%= max_tokens %></p>
	<p>Pre-prompt Text: <%= preprompttext %></p>
	<p>Last Messages: <%= lastmessages %></p> -->
	<div class="table-responsive">
		<table class="table" id="messagesTable">
		  <thead>
			<tr>
			  <th>Time</th>
			  <th>Persona</th>
			  <th>Message</th>
			  <th>Characters</th>
			  <th>Total_Tokens</th>
			  <th>Prompt_Tokens</th>
			  <th>Completion_Tokens</th>
			  <th>Max_Tokens</th>
			  <th>Preprompt_Text</th>
			  <th>Last_Messages</th>
			</tr>
		  </thead>
		  <tbody>
			<% messages.forEach(message => { %>
			  <tr>
				<td><%= message.time %></td>
				<td><%= message.persona %></td>
				<td><%= message.message %></td>
				<td><%= message.characters %></td>
				<td><%= message.total_tokens %></td>
				<td><%= message.prompt_tokens %></td>
				<td><%= message.completion_tokens %></td>
				<td><%= message.max_tokens %></td>
				<td title="<%= message.preprompttext %>"><%= message.preprompttext.slice(0, 25) %>...</td>
				<td title="<%= message.lastmessages %>"><%= message.lastmessages.slice(0, 25) %>...</td>
			  </tr>
			<% }); %>
		  </tbody>
		</table>
	  </div>
</body>
<script>
	$(document).ready(function() {
	  $('#messagesTable').DataTable();
	});
  </script>
</html>